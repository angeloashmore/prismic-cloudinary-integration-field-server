{
  "extends": "plugin:@typescript-eslint/recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "curly": "error",
    "prefer-const": "error",
    "newline-before-return": "error",
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" }
    ],
    "@typescript-eslint/member-delimiter-style": [
      "error",
      { "multiline": { "delimiter": "none" } }
    ],
    "@typescript-eslint/camelcase": "off"
  }
}
